#include<stdio.h>
void main()
{
    int n,i,j,k,l;
    char ch;
    printf("Enter height \n");
    scanf("%d",&n);
    for(i=1;i<=n;i++)
    {
        ch='A';
        for(j=n-i;j>0;j--)
        {
            printf(" ");
        }
        for(k=1;k<=i;k++)
        {
            printf("%c",ch);
        ch++;
        }
        ch=ch-2;
        for(l=1;l<=(i-1);l++)
        {
            printf("%c",ch);
            ch--;
        }
        printf("\n");
    }
}
